<Window x:Class="P3_Midwife.Views.RecordWindow"
        x:Name="RView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ViewModel="clr-namespace:P3_Midwife.ViewModel"
        xmlns:local="clr-namespace:P3_Midwife.Views" 
        mc:Ignorable="d"
        Title="RecordWindow" Height="720" Width="1280" Background="WhiteSmoke" BorderThickness="1" BorderBrush="LightSkyBlue">

    <Window.DataContext>
        <ViewModel:RecordViewModel/>
    </Window.DataContext>


    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="199*"/>
            <ColumnDefinition Width="410*"/>
            <ColumnDefinition Width="663*"/>
        </Grid.ColumnDefinitions>
        <Menu Width="50" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left">
            <MenuItem Header="Menu" >
                <MenuItem Command="{Binding BackCommand}" Header="Tilbage"/>
                <MenuItem Command="{Binding LogOutCommand}" Header="Log ud"/>
                <MenuItem Command="{Binding ExitCommand}" Header="Exit"/>
            </MenuItem>
        </Menu>

        <StackPanel Grid.ColumnSpan="3" Margin="0,10,0,-10">
            <ListBox x:Name="BirthInfo" Margin="199,20,0,0">
                <TextBlock x:Name="BItextBlock" TextWrapping="Wrap" Text="Fødselsoplysninger" />
            </ListBox>

            <ListView x:Name="BirthInfoHeader" Margin="199,0,0,0">
                <ListView.View>
                    <GridView>
                        <GridViewColumn>
                            <ListViewItem Content="Udførelse startet"/>
                        </GridViewColumn>
                        <GridViewColumn>
                            <ListViewItem Content="Fostervand"/>
                        </GridViewColumn>
                        <GridViewColumn>
                            <ListViewItem Content="Væskemængde"/>
                        </GridViewColumn>
                        <GridViewColumn>
                            <ListViewItem Content="Fødselsposition"/>
                        </GridViewColumn>
                        <GridViewColumn>
                            <ListViewItem Content="Blødningsårsag"/>
                        </GridViewColumn>
                        <GridViewColumn>
                            <ListViewItem Content="Mængde af blod"/>
                        </GridViewColumn>
                        <GridViewColumn>
                            <ListViewItem Content="Resultat"/>
                        </GridViewColumn>
                    </GridView>
                </ListView.View>

                <ListView ItemsSource="{Binding BirthInformationListProperty}" Margin="199,0,0,0">
                    <ListView.View>
                        <GridView>
                            <GridViewColumn>
                                <StackPanel>
                                    <ListView>
                                        <TextBox Text="{Binding BirthInfo.Time}"/>
                                    </ListView>
                                </StackPanel>
                            </GridViewColumn>
                            <GridViewColumn>
                                <StackPanel>
                                    <ListView ItemsSource="{Binding BirthInfo}">
                                        <TextBox Text="{Binding BirthInfo.AmnioticFluid}"/>
                                    </ListView>
                                </StackPanel>
                            </GridViewColumn>
                            <GridViewColumn>
                                <StackPanel>
                                    <ListView ItemsSource="{Binding BirthInfo}">
                                        <TextBox Text="{Binding BirthInfo.AmountOfFluid}"/>
                                    </ListView>
                                </StackPanel>
                            </GridViewColumn>
                            <GridViewColumn>
                                <StackPanel>
                                    <ListView ItemsSource="{Binding BirthInfo}">
                                        <TextBox Text="{Binding BirthInfo.BirthPosition}"/>
                                    </ListView>
                                </StackPanel>
                            </GridViewColumn>
                            <GridViewColumn>
                                <StackPanel>
                                    <ListView ItemsSource="{Binding BirthInfo}">
                                        <TextBox Text="{Binding BirthInfo.BleedingCause}"/>
                                    </ListView>
                                </StackPanel>
                            </GridViewColumn>
                            <GridViewColumn>
                                <StackPanel>
                                    <ListView ItemsSource="{Binding BirthInfo}">
                                        <TextBox Text="{Binding BirthInfo.BloodAmount}"/>
                                    </ListView>
                                </StackPanel>
                            </GridViewColumn>
                            <GridViewColumn>
                                <StackPanel>
                                    <ListView ItemsSource="{Binding BirthInfo}">
                                        <TextBox Text="{Binding BirthInfo.Result}"/>
                                    </ListView>
                                </StackPanel>
                            </GridViewColumn>
                            <GridViewColumn>
                                <Button Command="{Binding AddBirthInfo}">Gem</Button>
                            </GridViewColumn>
                        </GridView>
                    </ListView.View>
                </ListView>




