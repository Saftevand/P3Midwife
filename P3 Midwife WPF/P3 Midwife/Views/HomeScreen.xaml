<Window x:Class="P3_Midwife.HomeScreen"
        x:Name="HScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:P3_Midwife"
        xmlns:ViewModel="clr-namespace:P3_Midwife"
        mc:Ignorable="d"
        Title="Menu" Height="300" Width="300">
        <Window.DataContext>
            <ViewModel:HomeScreenViewModel/>
        </Window.DataContext>
    <Grid>
        <StackPanel Height="280" Width="200">
            <TextBox Text="{Binding CPR, UpdateSourceTrigger=LostFocus}"></TextBox>
            <Button Command="{Binding }" Margin="5">FindPatient
            </Button>
        </StackPanel>
           
        <Menu Width="50" Height="20">
            <MenuItem Header="Menu">
                <MenuItem Command="{Binding LogOutCommand}" Header="Log ud"/>
                <MenuItem Header="Exit"/>
            </MenuItem>
        </Menu>
    </Grid>
</Window>

